# Пример конфигурации wg-obfuscator для OpenWRT
# Скопируйте этот файл в /etc/wg-obfuscator.conf и отредактируйте под свои нужды

# Основная секция
[main]

# Интерфейс для прослушивания (0.0.0.0 = все интерфейсы)
source-if = 0.0.0.0

# Порт для прослушивания (WireGuard должен подключаться к этому порту)
source-lport = 13255

# Целевой сервер и порт (реальный WireGuard сервер)
target = your-server.com:13255

# Ключ обфускации (должен совпадать на обеих сторонах)
key = your-secret-key-here

# Тип маскировки для обхода DPI
# NONE - без маскировки
# AUTO - автоматическое определение (рекомендуется)
# STUN - маскировка под STUN протокол
masking = AUTO

# Уровень детализации логов
# ERRORS - только критические ошибки
# WARNINGS - важные сообщения
# INFO - информационные сообщения (рекомендуется)
# DEBUG - подробные отладочные сообщения
# TRACE - очень подробные сообщения с дампами пакетов
verbose = INFO

# Максимальное количество клиентов
max-clients = 1024

# Таймаут неактивности в секундах
idle-timeout = 300

# Максимальная длина фиктивных данных для пакетов (0-1024 байта)
max-dummy-length-data = 4

# Firewall mark для предотвращения маршрутных петель (0 = отключено)
fwmark = 0

# Статические привязки для двунаправленного режима
# Формат: client_ip:client_port:forward_port
# static-bindings = 1.2.3.4:12883:6670, 5.6.7.8:12083:6679

# Дополнительные секции для множественных экземпляров
# [server2]
# source-if = 0.0.0.0
# source-lport = 13256
# target = another-server.com:13255
# key = different-secret-key
# masking = STUN
# verbose = DEBUG